<div class="col-md-4">
  <div class="btn-group">
    <button class="btn btn-default btn-sm dropdown-toggle" type="button" data-toggle="dropdown" aria-expanded="false">
      <%= sym.historical_api || 'Select API' %> <span class="caret"></span>
    </button>
    <ul class="dropdown-menu" role="menu">
      <% Api.all.each do |api| %>
        <% next if api == sym.historical_api %>
        <li><%= link_to "Run with #{api}", rerun_historical_sym_path(sym, api_id: api.id) %></li>
      <% end %>
    </ul>
  </div>
</div>
